      <name>Public Snapshots</name>
      <url>http://nexus.hc.to/content/repositories/pub_snapshots/</url>
    </snapshotRepository>
  </distributionManagement>
  <properties>
    <java.version>21</java.version>
    <project.build.sourceEncoding>UTF-8</project.build.sourceEncoding>
  </properties>
</project>
